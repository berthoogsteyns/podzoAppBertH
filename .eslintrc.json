{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "jsx": true,
    "useJSXTextNode": true
  },
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"]

  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "plugins": ["@typescript-eslint"]
}
